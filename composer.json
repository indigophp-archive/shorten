{
    "name": "indigophp/shorten",
    "description": "Shorten your URLs using various providers",
    "license": "MIT",
    "keywords": ["url", "shorten", "google", "bit.ly", "ow.ly", "tiny.cc"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "indigophp/http-adapter": "dev-develop",
        "league/url": "~3.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~5.0",
        "phpspec/phpspec": "~2.1.0",
        "henrikbjorn/phpspec-code-coverage" : "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Shorten\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
